{
  "name": "nunjucks",
  "version": "4.0.0-beta.1",
  "description": "A powerful templating engine with inheritance, asynchronous control, and more (jinja2 inspired)",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "build": "yarn run build:rollup && yarn run build:babel",
    "build:rollup": "rollup -c",
    "build:babel": "babel src/ --out-dir lib/",
    "instrument": "cross-env NODE_ENV=test rollup -c",
    "watch": "concurrently \"yarn run watch:rollup\" \"yarn run watch:babel\" \"yarn run watch:instrument\"",
    "watch:rollup": "rollup -c -w",
    "watch:babel": "babel -w src/ --out-dir lib/",
    "watch:instrument": "cross-env NODE_ENV=test rollup -c -w"
  },
  "bin": {
    "nunjucks-precompile": "./bin/precompile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/nunjucks.git"
  },
  "author": "Frankie Dintino <fdintino@gmail.com> (https://www.frankiedintino.com/)",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/mozilla/nunjucks/issues"
  },
  "homepage": "https://github.com/mozilla/nunjucks#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "a-sync-waterfall": "^1.0.0",
    "asap": "^2.0.3",
    "es6-symbol": "^3.1.2",
    "events": "^3.0.0",
    "pregenerator": "^1.0.0-beta.3",
    "regenerator-runtime": "^0.13.3",
    "yargs": "^14.2.0"
  },
  "devDependencies": {},
  "keywords": [
    "template",
    "templating"
  ],
  "publishConfig": {
    "access": "public"
  }
}
