{
  "name": "nunjucks",
  "version": "4.0.0-beta.1",
  "description": "A powerful templating engine with inheritance, asynchronous control, and more (jinja2 inspired)",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "build": "yarn run build:rollup && yarn run build:babel",
    "build:rollup": "rollup -c",
    "build:babel": "babel src/ --out-dir lib/",
    "instrument": "yarn run instrument:rollup && yarn run instrument:babel",
    "instrument:rollup": "cross-env NODE_ENV=test rollup -c",
    "instrument:babel": "cross-env NODE_ENV=test babel src/ --out-dir test/lib",
    "watch": "concurrently \"yarn run watch:rollup\" \"yarn run watch:babel\" \"yarn run watch:instrument\"",
    "watch:rollup": "rollup -c -w",
    "watch:babel": "babel -w src/ --out-dir lib/",
    "watch:instrument": "cross-env NODE_ENV=test rollup -c -w"
  },
  "bin": {
    "nunjucks-precompile": "./bin/precompile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/nunjucks.git"
  },
  "author": "Frankie Dintino <fdintino@gmail.com> (https://www.frankiedintino.com/)",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/mozilla/nunjucks/issues"
  },
  "homepage": "https://github.com/mozilla/nunjucks#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "a-sync-waterfall": "^1.0.0",
    "asap": "^2.0.3",
    "es6-symbol": "^3.1.2",
    "events": "^3.0.0",
    "pregenerator": "^1.0.0-beta.3",
    "regenerator-runtime": "^0.13.3",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "ast-types": "^0.13.1",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "concurrently": "^4.1.1",
    "connect": "^3.7.0",
    "core-js": "2",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "get-port": "^5.0.0",
    "istanbul-lib-instrument": "^3.3.0",
    "lerna": "^3.15.0",
    "mocha": "^6.1.4",
    "mocha-phantomjs-core": "^2.1.2",
    "mocha-phantomjs-istanbul": "^0.0.2",
    "ncp": "^2.0.0",
    "nyc": "^14.1.1",
    "phantomjs-prebuilt": "^2.1.16",
    "promise": "^8.0.3",
    "promise-polyfill": "^8.1.3",
    "puppeteer": "^1.17.0",
    "recast": "^0.18.1",
    "rollup": "^1.15.6",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-resolve-id": "^1.0.2",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-pluginutils": "^2.8.1",
    "serve-static": "^1.14.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "uglify-js": "^3.6.0"
  },
  "keywords": [
    "template",
    "templating"
  ],
  "publishConfig": {
    "access": "public"
  }
}
